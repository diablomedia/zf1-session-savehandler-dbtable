{
    "name": "diablomedia/zendframework1-session-savehandler-dbtable",
    "description": "Zend Framework 1 Session_SaveHandler_DbTable component",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "prefer-stable": true,
    "require": {
        "ext-session": "*",
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0 || ~8.5.0",
        "diablomedia/zendframework1-config": "^2.0.5",
        "diablomedia/zendframework1-session": "^2.0.0",
        "diablomedia/zendframework1-db": "^1.0.5"
    },
    "autoload": {
        "classmap": [
            "src/Zend/Session/SaveHandler/DbTable.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Zend_": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.46",
        "phpstan/phpstan": "2.1.33",
        "friendsofphp/php-cs-fixer": "3.93.0",
        "maglnet/composer-require-checker": "^3.0.0",
        "phpro/grumphp-shim": "^2.0.0"
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test-with-coverage": "phpunit --coverage-clover=clover.xml",
        "phpstan": "phpstan analyze -c phpstan.neon --no-progress --ansi",
        "style-check": "php-cs-fixer fix --dry-run -vv"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
